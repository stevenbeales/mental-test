packages:
  yum:
    postgresql93-devel: []
    
files:
    "/etc/httpd/conf.d/ssl_rewrite.conf":
        mode: "000644"
        owner: root
        group: root
        content: |
            RewriteEngine On
            RewriteCond %{HTTP:X-Forwarded-Proto} !https
            RewriteCond %{HTTP_USER_AGENT} !^ELB-HealthChecker.* 
            RewriteRule . https://%{SERVER_NAME}%{REQUEST_URI} [L,R]
